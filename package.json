  "scripts": {
    "postinstall": "bun run generate-favicons",
    "dev": "astro dev",
    "prebuild": "move-file ./src/pages/pdf.astro ./src/pages/_pdf.astro && bun run generate-favicons",
    "build": "astro build",
    "postbuild": "move-file ./src/pages/_pdf.astro ./src/pages/pdf.astro",
    "preview": "astro preview",
